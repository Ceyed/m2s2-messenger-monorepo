{
  "name": "frontend-m2s2",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/frontend/m2s2-web/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/rspack:rspack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "web",
        "outputPath": "dist/apps/frontend/m2s2-web",
        "main": "apps/frontend/m2s2-web/src/main.tsx",
        "tsConfig": "apps/frontend/m2s2-web/tsconfig.app.json",
        "rspackConfig": "apps/frontend/m2s2-web/rspack.config.js",
        "assets": ["apps/frontend/m2s2-web/src/favicon.ico", "apps/frontend/m2s2-web/src/assets"]
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/frontend/m2s2-web/src/environments/environment.ts",
              "with": "apps/frontend/m2s2-web/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/rspack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "frontend-m2s2:build",
        "hmr": false
      },
      "configurations": {
        "development": {
          "buildTarget": "frontend-m2s2:build:development"
        },
        "production": {
          "buildTarget": "frontend-m2s2:build:production",
          "hmr": false
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "frontend-m2s2:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/frontend/m2s2-web/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
